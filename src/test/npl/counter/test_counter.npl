package counter;

@test
function initCounter(test: Test) -> {
    var counter: Counter = Counter['user'](5);
    test.assertEquals(5, counter.target);
    test.assertEquals(0, counter.i);
    test.assertEquals(Counter.States.counting, counter.activeState().getOrFail());
};

@test
function increment(test: Test) -> {
    var counter: Counter = Counter['user'](5);
    test.assertEquals(0, counter.i);
    counter.increment['user']();
    test.assertEquals(1, counter.i);
};
