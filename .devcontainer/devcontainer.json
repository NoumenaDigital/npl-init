{
  "name": "NPL Development Environment",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "noumenadigital.npl-dev-vscode-extension"
      ]
    }
  },
  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:2": {
      "version": "latest"
    }
  },
  "remoteUser": "codespace",
  "containerUser": "codespace"
}
